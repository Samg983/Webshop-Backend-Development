<?php

class Tag{
    private $tagId;
    private $tagNaam;
    
    function getTagId() {
        return $this->tagId;
    }

    function getTagNaam() {
        return $this->tagNaam;
    }

    function setTagId($tagId) {
        $this->tagId = $tagId;
    }

    function setTagNaam($tagNaam) {
        $this->tagNaam = $tagNaam;
    }

    function __construct($tagId, $tagNaam) {
        $this->tagId = $tagId;
        $this->tagNaam = $tagNaam;
    }
}

?>
