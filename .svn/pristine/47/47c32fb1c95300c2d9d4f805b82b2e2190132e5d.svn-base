<?php
class Categorie {
    private $categorieId;
    private $naam;
    
    function getCategorieId() {
        return $this->categorieId;
    }

    function getNaam() {
        return $this->naam;
    }

    function setCategorieId($categorieId) {
        $this->categorieId = $categorieId;
    }

    function setNaam($naam) {
        $this->naam = $naam;
    }


    function __construct($categorieId, $naam) {
        $this->categorieId = $categorieId;
        $this->naam = $naam;
    }
}
?>
