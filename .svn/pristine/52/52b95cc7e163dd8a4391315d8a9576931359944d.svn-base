<?php

class ProductTag{
    private $productTagId;
    private $tagId;
    private $productId;
    
    function getProductTagId() {
        return $this->productTagId;
    }

    function setProductTagId($productTagId) {
        $this->productTagId = $productTagId;
    }
   
    function getTagId() {
        return $this->tagId;
    }

    function getProductId() {
        return $this->productId;
    }

    function setTagId($tagId) {
        $this->tagId = $tagId;
    }

    function setProductId($productId) {
        $this->productId = $productId;
    }

    function __construct($productTagId, $tagId, $productId) {
        $this->productTagId = $productTagId;
        $this->tagId = $tagId;
        $this->productId = $productId;
    }
}
?>